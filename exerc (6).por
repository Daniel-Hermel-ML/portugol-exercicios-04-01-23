programa {
  /*Exercício 6*/
  funcao inicio() {
    inteiro num, val = 0
      faca   
      {  
        escreva ("--Digite um número: ")  
        leia (num) 
        se(num >= 1)
        {
          val = num + val
        }
      } enquanto (num != 0) 
        escreva("A soma dos números positivos é: "+val)
      }
    }   
  }
}
